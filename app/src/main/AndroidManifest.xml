        </activity>
        <activity android:name=".scratch.ui.ScratchActivity"></activity>
        <activity android:name=".dashboard.ui.DashboardDemoActivity"></activity>
        <activity android:name=".progress.ui.ProgressCircleActivity"></activity>
        <activity android:name=".nestedsroll.ui.NestedScrollingCustomActivity"></activity>
    </application>

</manifest>